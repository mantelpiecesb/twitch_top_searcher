<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/my_nav_graph"
    xmlns:tools="http://schemas.android.com/tools"
    app:startDestination="@id/mainActivity">

    <activity
        android:id="@+id/mainActivity"
        android:name="com.example.android.codelabs.paging.ui.SearchTwitchTopsActivity"
        android:label="activity_main"
        tools:layout="@layout/activity_search_twitch_tops"
        >
        <action
            android:id="@+id/action_mainActivity_to_feedbackActivity"
            app:destination="@id/feedbackActivity" />
    </activity>

    <activity
        android:id="@+id/feedbackActivity"
        android:name="com.example.android.codelabs.paging.ui.FeedbackActivity"
        android:label="activity_feedback"
        tools:layout="@layout/activity_feedback" />
</navigation>